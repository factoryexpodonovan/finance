
<%= bootstrap_form_for(@finance_customer) do |f| %>
	<fieldset>
		<legend>Member Info</legend>
		<%= f.text_field :name, :help_inline => 'Login Name'%>
		<%= f.email_field :email %>
		<%= f.text_field :password %>
		<%= f.text_field :password_confirmation %>
		<%= f.actions %>
	</fieldset>

<% end %>



