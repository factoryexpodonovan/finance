<% loaded_page = controller.controller_name + ' ' + controller.action_name %>
<% if loaded_page != 'static_pages home' %>

<div class='navbar'>
	<div class="navbar-inner">
		
			<a class="brand pull-left" href="#">Finance Prospects</a>
			
				<ul class="nav">
					<% if signed_in? %>
						<li><a href="#">Signed in as <strong><%= current_finance_customer.name %></strong></a></li>
						<li><a href="#">My Account</a></li>
						<li>
							<%= link_to "Sign Out", signout_path, method: "delete"%>
						</li>
						
					<% else %>
						<li ></li>
					<% end %>
					<% if current_finance_customer.admin? %>
						<li class="dropdown pull-right">
							admin
						</li>
					<% end %>
				</ul>
						
				
			
		
	</div>
</div>
<%	 end %>